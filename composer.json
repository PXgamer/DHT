{
    "name": "pxgamer/dht-search",
    "type": "library",
    "description": "A collection of tools for searching DHT via PHP.",
    "license": "MIT",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\DHT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\DHT\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
